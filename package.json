    "type": "git",
    "url": "git+ssh://git@github.com/orangecoding/homebridge-influx.git"
  },
  "dependencies": {
    "influx": "5.7.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "homebridge": "^1.3.2",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "lint": "^0.7.0"
  },
  "bugs": {
    "url": "https://github.com/orangecoding/homebridge-influx/issues"
